<tr>
  <td><%= ticket.id %></td>
  <td><%= ticket.train.number %></td>
  <td><%= ticket.start_station.title %></td>
  <td><%= ticket.end_station.title %></td>
  <td><%= ticket.user.name if ticket.user.present? %></td>
  <td><%= ticket.user_name %></td>
  <td><%= link_to 'Просмотр', [:admin, ticket] %></td>
  <td><%= link_to 'Изменить', edit_admin_ticket_path(ticket) %></td>
  <td><%= link_to 'Удалить', [:admin, ticket], method: :delete, data: {confirm: 'Вы уверены?'} %></td>
</tr>